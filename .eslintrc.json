{
  "parser": "babel-eslint",
  "extends": [
    "react-app",
    "prettier"
  ],
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "settings": {
    "react": {
      "pragma": "React",
      "version": "16.8.6"
    }
  },
  "rules": {
    "indent": [
      "error",
      2
    ],
    "jsx-quotes": [
      1,
      "prefer-double"
    ],
    "jsx-a11y/anchor-is-valid": "off"
  },
  "plugins": [
    "prettier"
  ]
}
